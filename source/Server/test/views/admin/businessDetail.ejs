<!DOCTYPE HTML>
<html xmlns="http://www.w3.org/1999/html">
<head>
    <title>Business_details List</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="keywords" content="" />
    <script type="application/x-javascript"> addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false); function hideURLbar(){ window.scrollTo(0,1); } </script>
    <!-- Bootstrap Core CSS -->
    <link href="css/bootstrap.min.css" rel='stylesheet' type='text/css' />
    <!-- Custom CSS -->
    <link href="css/style-administrator.css" rel='stylesheet' type='text/css' />
    <link href="css/lib/font-awesome.css" rel="stylesheet">
    <!-- jQuery -->
    <script src="js/js-administrator/jquery.min.js"></script>
    <!----webfonts--->
    <link href="css/useso.css" rel='stylesheet' type='text/css'>
    <!---//webfonts--->
    <!-- Bootstrap Core JavaScript -->
    <script src="js/bootstrap.min.js"></script>
</head>
<body>
<div id="wrapper">
    <!-- Navigation -->
    <nav class="top1 navbar navbar-default navbar-static-top" role="navigation" style="margin-bottom: 0">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <a class="navbar-brand" href="/admin">食友商家</a>
        </div>
        <!-- /.navbar-header -->
        <form class="welcome" style="text-align:right">
            <p>管理员，</p></br>
            <p>欢迎回来</p>
        </form>
        <div class="navbar-default sidebar" role="navigation">
            <div class="sidebar-nav navbar-collapse">
                <ul class="nav" id="side-menu">
                    <li>
                        <a href="/admin"><img src="images/icon/home1.jpg" alt="" />Home</a>
                    </li>
                    <li>
                        <a href="#"><img src="images/icon/shangjia1.jpg" alt="" />商户信息<span class="fa arrow"></span></a>
                        <ul class="nav nav-second-level">
                            <li>
                                <a href="/admin_business">商户列表</a>
                            </li>
                            <li>
                                <a href="/admin_newBusiness">新入驻商户列表</a>
                            </li>
                        </ul>
                        <!-- /.nav-second-level -->
                    </li>
                    <li>
                        <a href="/admin_detail"><img src="images/icon/administrator.jpg" alt="" />管理员信息</a>
                    </li>
                    <li>
                        <a href="/logout"><img src="images/icon/logout.jpg" alt="" />Log Out</a>
                    </li>
                </ul>
            </div>
            <!-- /.sidebar-collapse -->
        </div>
        <!-- /.navbar-static-side -->
    </nav>
    <div id="page-wrapper">
        <div class="graphs">
            <div class="xs">
                <h3>商家详情</h3>
                <div class="tab-content">
                    <div class="tab-pane active" id="horizontal-form">
                        <form class="form-horizontal">
                            <div class="form-group">
                                <label for="disabledinput" class="col-sm-2 control-label">商家名称</label>
                                <div class="col-sm-8">
                                    <input disabled="" type="text" class="form-control1" id="sellername" placeholder=<%= rows[0].res_name %>>
                                </div>
                            </div>
                            <div class="form-group">
                                <label for="disabledinput" class="col-sm-2 control-label">商家简介</label>
                                <div class="col-sm-8">
                                    <textarea disabled="disabled" rows="3" cols="60" class="form-control1" id="sellerbrief"><%= rows[0].introduction %></textarea>
                                </div>
                            </div>
                            <div class="form-group">
                                <label for="disabledinput" class="col-sm-2 control-label">法人姓名</label>
                                <div class="col-sm-8">
                                    <input disabled="" type="text" class="form-control1" id="realname" placeholder=<%= rows[0].manager_name %>>
                                </div>
                            </div>
                            <div class="form-group">
                                <label for="disabledinput" class="col-sm-2 control-label">身份证号</label>
                                <div class="col-sm-8">
                                    <input disabled="" type="text" class="form-control1" id="idcard" placeholder=<%= rows[0].manager_idnum %>>
                                </div>
                            </div>
                            <div class="form-group">
                                <label for="disabledinput" class="col-sm-2 control-label">地址</label>
                                <div class="col-sm-8">
                                    <input disabled="" type="text" class="form-control1" id="address" placeholder="<%= rows[0].address %>">
                                </div>
                            </div>
                            <div class="form-group">
                                <label for="disabledinput" class="col-sm-2 control-label">营业时间</label>
                                <div class="col-sm-8">
                                    <input disabled="" type="text" class="form-control1" id="business_hours" placeholder="<%= rows[0].business_hour %>" >
                                </div>
                            </div>
                            <div class="form-group">
                                <label for="disabledinput" class="col-sm-2 control-label">地域</label>
                                <div class="col-sm-8">
                                    <input disabled="" type="text" class="form-control1" id="field" placeholder="北京市海淀区">
                                </div>
                            </div>
                            <div class="form-group">
                                <label for="disabledinput" class="col-sm-2 control-label">标签</label>
                                <div class="col-sm-8">
                                    <input disabled="" type="text" class="form-control1" id="sellerlabel" placeholder="海鲜自助">
                                </div>
                            </div>
                            <div class="form-group">
                                <label for="disabledinput" class="col-sm-2 control-label">信用评级</label>
                                <div class="col-sm-8">
										<span class="stars" id="creditrating">
											<%= rows[0].star %>
										</span>
                                </div>
                            </div>
                            <form role="form" class="form-horizontal">
                                <div class="form-group">
                                    <label class="col-md-2 control-label">联系电话</label>
                                    <div class="col-md-8">
                                        <div class="input-group">
												<span class="input-group-addon">
													<i class="fa fa-fax"></i>
												</span>
                                            <input disabled="" type="text" class="form-control1" id="phonenumber" placeholder=<%= rows[0].phone %>>
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-md-2 control-label">邮箱地址</label>
                                    <div class="col-md-8">
                                        <div class="input-group">
												<span class="input-group-addon">
													<i class="fa fa-envelope-o"></i>
												</span>
                                            <input disabled="" type="text" class="form-control1" id="email" placeholder=<%= rows[0].email %>>
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="disabledinput" class="col-sm-2 control-label">商家图片</label>
                                    <div class="col-sm-8">
                                        <div class="col-md-4 widget_1_box1">
                                            <table width="500" height="100" border="0">
                                                <tr>
                                                    <td width="250" height="100" align="left" id="recipeimg1"><img class="img-responsive" src="<%= rows[0].pic %>" border="0"></td>
                                                </tr>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </form>
                    </div>
                </div>

                <div class="bs-example4" data-example-id="contextual-table">
                    <h3>菜单列表</h3>
                    <table class="table">
                        <thead>
                        <tr>
                            <th>序号</th>
                            <th>菜品名称</th>
                            <th>菜品口味</th>
                            <th>菜品价格</th>
                            <th>推荐菜</th>
                        </tr>
                        </thead>
                        <tbody>
                        <% if(dishRows.length > 0){%>
                            <% for(var i = 0, len = dishRows.length; i < len; i ++ ){%>
                                <% var arr = new Array("active", "success", "info", "warning", "danger") %>
                                    <% if( i%2 == 0){%>
                                    <tr class= "<%= arr[i%5] %>" >
                                        <% }else{ %>
                                    <tr class= "">
                                        <% } %>
                                <!--<tr class="active">-->
                                    <th scope="row"><%= (i+1) %></th>
                                    <td><a href="administrator_businessrecipe.html"><%= dishRows[i].name %></a></td>
                                    <td><%= dishRows[i].flavour %></td>
                                    <td><%= dishRows[i].price %></td>
                                    <% if(dishRows[i].isRecommend){ %>
                                        <td>是</td>
                                    <% }else{ %>
                                        <td>否</td>
                                    <% } %>
                                </tr>
                            <% } %>
                        <% } %>
                        </tbody>
                    </table>
                    <ul class="pager">
                        <li><a href="#" input type="button">←</a></li>
                        <li><a href="#" input type="button">→</a></li>
                    </ul>
                </div>

            </div>
        </div>
        <div class="copy_layout">
            <p>&copy; Summer Cooperation 2016</p>
        </div>
    </div>
</div>
<!-- /#page-wrapper -->
</div>
<!-- /#wrapper -->
<!-- Nav CSS -->
<link href="css/css-administrator/custom.css" rel="stylesheet">
<!-- Metis Menu Plugin JavaScript -->
<script src="js/js-administrator/metisMenu.min.js"></script>
<script src="js/js-administrator/custom.js"></script>
</body>
</html>
